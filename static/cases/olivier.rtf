\par {\listtext\pard\plain\ltrpar \s17 \rtlch\fcs1 \af31507 \ltrch\fcs0 \f39 \hich\af39\dbch\af31506\loch\f39 9.\tab}}\pard\plain \ltrpar\s17\qj \fi-480\li720\ri0\sa240\widctlpar\wrapdefault\aspalpha\aspnum\faauto\ls17\adjustright\rin0\lin720\itap0 
\rtlch\fcs1 \af31507\afs24\alang1025 \ltrch\fcs0 \f39\fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid7234745 Permitted Uses}{\rtlch\fcs1 \af31507 \ltrch\fcs0 \insrsid12726989 
